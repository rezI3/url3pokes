// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
  previewFeatures = ["referentialIntegrity"] //　planet scale特有
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  // relationMode = "prisma"
  referentialIntegrity = "prisma" // planeScale特有
}

model url3pokes {
  first  String
  second String
  third  String
  url    String @unique

  @@id([first, second, third])
}
